@model IEnumerable<Invite>

<article class="center">
  <h1>Invitation</h1>

  <section class="center border">
    <p>
      Do you want come up ?
      <br>
      Then go by this ref.
    </p>

    <a class="link" asp-action="Form">Here</a>
  </section>
  <section class="center border guest-list">
    <p>Look how much people come up!</p>

    <table class="table">
      <thead>
        <tr>
          <th>Name</th>
          <th>LastName</th>
          <th>Phone</th>
          <th>Email</th>
        </tr>
      </thead>
      <tbody>
        @foreach (Invite u in Model)
        {
          <tr>
            <td>@u.Name</td>
            <td>@u.LastName</td>
            <td>@u.Phone</td>
            <td>@u.Email</td>
          </tr>
        }
      </tbody>
    </table>
  </section>
</article>
